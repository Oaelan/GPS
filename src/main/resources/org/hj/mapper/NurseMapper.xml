<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="org.hj.mapper.NurseMapper">

	<!-- 환자 리스트 가져옴 -->
	<select id="getPList" resultType="org.hj.model.PatientVO" parameterType="org.hj.model.UserVO">
		select p_no,p_name
		from patient
		where s_id = #{id}
	</select>



</mapper>